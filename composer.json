{
  "name": "treolabs/pim",
  "description": "TreoPIM. Open source PIM application.",
  "homepage": "https://treopim.com",
  "license": "GPL-3.0-only",
  "require": {
    "treolabs/treocore": "~3.23.25",
    "treolabs/colored-fields": "~1.7.13",
    "treolabs/multi-languages": "~1.13.0",
    "treolabs/import-feeds-common": "~1.3.0"
  },
  "autoload": {
    "psr-4": {
      "Pim\\": "app/"
    }
  },
  "extra": {
    "treoId": "Pim",
    "name": {
      "default": "PIM"
    },
    "description": {
      "default": "PIM module for Treo Core."
    }
  }
}
