{
  "name": "kennersoft/pim",
  "description": "KennerPim. Open source PIM application.",
  "homepage": "https://kennerpim.de",
  "license": "GPL-3.0-only",
  "require": {
    "kennersoft/kennercore": ">=3.23.39 <3.25.0",
    "kennersoft/colored-fields": "~1.7.13",
    "kennersoft/multi-languages": "~1.13.0",
    "kennersoft/import-feeds-common": "~1.3.0"
  },
  "autoload": {
    "psr-4": {
      "Pim\\": "app/"
    }
  },
  "extra": {
    "treoId": "Pim",
    "name": {
      "default": "PIM"
    },
    "description": {
      "default": "PIM module for KennerCore."
    }
  }
}
